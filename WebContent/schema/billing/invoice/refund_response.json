{
	"$schema": "http://koderlabs.com/myschema#",
	"type": "object",
	"title": "Refund Response",
	"properties": 
	{
		"requestId": 
		{
			"title": "Request Id",
			"type": "integer"
		},
		"action": 
		{
			"title": "Refund Action",
			"enum": 
			[
				"refund_approved",
				"refund_rejected"
			]
		},
		"description": 
		{
			"title": "Refund Reason",
			"type": "string"
		}
	},

	"required": 
	[
		"requestId",
		"action"
	]
}